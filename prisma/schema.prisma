// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String 
  age Int 
}

model Car {
  id Int @id @default(autoincrement())
  marca String
}

model boy{
  id Int @id @default(autoincrement())
  name String @unique
}